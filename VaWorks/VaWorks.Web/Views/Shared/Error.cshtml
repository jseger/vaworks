@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

<h1 class="text-danger">Uh Oh!</h1>
<h2 class="text-danger">Well this is embarrassing.</h2>
<p class="text-danger">An error has occured with the application. We're terribly sorry about this and are working to resolve this issue.</p>
<p class="text-danger">@ViewBag.Error</p>

